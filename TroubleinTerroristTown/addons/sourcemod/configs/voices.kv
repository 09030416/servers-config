"Kxnrl.Vocies"
{
    // write example
    "Lilia.Arlene"
    {
        // general key
        "model" "model/player/custom_players/maoling/bullet_impact3/lilia/normal/lilia.mdl"

        // round event voice here
        "round"
        {
            "RS" "maoling/voice/rsvcr/lilia.mp3"
            "RW" "maoling/voice/rwvcr/lilia.mp3"
            "RL" "maoling/voice/rlvcr/lilia.mp3"
            "RD" "maoling/voice/rdvcr/lilia.mp3"
        }

        // player event voice here
        "event"
        {
            // idle
            "idles" "maoling/voice/idles/lilia.mp3"

            // spawn -> 0.5 sec delay
            "spawn" "maoling/voice/spawn/lilia.mp3"

            // death -> keep null built-in store.
            "death" "maoling/voice/death/lilia.mp3"

            // headshot 
            "hshot" "maoling/voice/hshot/lilia.mp3"

            // knockout 
            "knock" "maoling/voice/knock/lilia.mp3"

            // world damage or self damage
            "world" "maoling/voice/world/lilia.mp3"

            "hurts"
            {
                // randomly hurts voice group
                "maoling/voice/hurts/lilia_0.mp3"
                "maoling/voice/hurts/lilia_1.mp3"

                // +++++++++++++++ can more within arrayList

            }
        }
    }
}